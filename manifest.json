{
    "manifest_version": 2,
    "name": "Home Screen Project",
    "description": "aesthetic home screen and gratitude journal",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
      },
    "permissions": [
      "tabs",
      "downloads"
    ]
  }